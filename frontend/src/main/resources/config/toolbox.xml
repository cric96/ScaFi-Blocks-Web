<xml xmlns="https://developers.google.com/blockly/xml" id="toolbox" style="display: none">
    <category name="Basic" colour="#a5745b">
        <block type="output"></block>
    </category>
    <category name="Aggregate" colour="#9fa55b">
        <block type="nbr"></block>
        <block type="branch"></block>
        <block type="mux"></block>
        <block type="rep">
            <field name="PREVIOUS" id="yANW!y:.,l;R6;+zv.#?">x</field>
            <value name="EVOLUTION">
                <block type="lambda_1_param">
                    <field name="PARAM_NAME">prev</field>
                </block>
            </value>
        </block>
    </category>
    <category name="Utilities" colour="#5ba55b">
        <block type="mid"></block>
        <block type="nbrrange"></block>
        <block type="distance_to">
            <value name="SRC">
                <block type="sense">
                    <value name="TYPE">
                        <block type="class_boolean">
                            <field name="NAME">Boolean</field>
                        </block>
                    </value>
                    <value name="SENSOR_NAME">
                        <block type="string_value">
                            <field name="VALUE">source</field>
                        </block>
                    </value>
                </block>
            </value>
        </block>
        <block type="channel">
            <value name="SOURCE">
                <block type="sense">
                    <value name="TYPE">
                        <block type="class_boolean">
                            <field name="NAME">Boolean</field>
                        </block>
                    </value>
                    <value name="SENSOR_NAME">
                        <block type="string_value">
                            <field name="VALUE">source</field>
                        </block>
                    </value>
                </block>
            </value>
            <value name="TARGET">
                <block type="sense">
                    <value name="TYPE">
                        <block type="class_boolean">
                            <field name="NAME">Boolean</field>
                        </block>
                    </value>
                    <value name="SENSOR_NAME">
                        <block type="string_value">
                            <field name="VALUE">target</field>
                        </block>
                    </value>
                </block>
            </value>
        </block>
        <block type="distance_between">
            <value name="SOURCE">
                <block type="sense">
                    <value name="TYPE">
                        <block type="class_boolean">
                            <field name="NAME">Boolean</field>
                        </block>
                    </value>
                    <value name="SENSOR_NAME">
                        <block type="string_value">
                            <field name="VALUE">source</field>
                        </block>
                    </value>
                </block>
            </value>
            <value name="TARGET">
                <block type="sense">
                    <value name="TYPE">
                        <block type="class_boolean">
                            <field name="NAME">Boolean</field>
                        </block>
                    </value>
                    <value name="SENSOR_NAME">
                        <block type="string_value">
                            <field name="VALUE">target</field>
                        </block>
                    </value>
                </block>
            </value>
        </block>
    </category>
    <category name="Fold" colour="#5ba58c">
        <block type="foldhood">
            <value name="AGGREGATOR">
                <block type="lambda_2_param">
                    <field name="PARAM_1_NAME">x</field>
                    <field name="PARAM_2_NAME">y</field>
                </block>
            </value>
        </block>
        <block type="foldhoodplus">
            <value name="AGGREGATOR">
                <block type="lambda_2_param">
                    <field name="PARAM_1_NAME">x</field>
                    <field name="PARAM_2_NAME">y</field>
                </block>
            </value>
        </block>
        <block type="minhood">
        </block>
        <block type="minhoodplus">
        </block>
    </category>
    <category name="Sensors" colour="#5b80a5">
        <block type="sense">
            <value name="TYPE">
                <block type="class_boolean">
                    <field name="NAME">Boolean</field>
                </block>
            </value>
            <value name="SENSOR_NAME">
                <block type="string_value">
                    <field name="VALUE">sensorX</field>
                </block>
            </value>
        </block>
    </category>
    <category name="Actuators" colour="#5b67a5">
        <block type="led_all_to">
            <value name="COLOR">
                <block type="color_value">
                    <field name="VALUE">#ffff00</field>
                </block>
            </value>
        </block>
    </category>
    <category name="Operations" colour="#745ba5">
        <block type="equals"></block>
        <block type="not"></block>
        <block type="boolean_operation">
            <field name="OPERATOR">&amp;&amp;</field>
        </block>
        <block type="number_compare">
            <field name="OPERATOR">GREATER</field>
        </block>
        <block type="number_operation">
            <field name="OPERATOR">ADDITION</field>
        </block>
        <block type="ternary_operation">
        </block>
    </category>
    <category name="Values" colour="#995ba5">
        <block type="string_value">
            <field name="VALUE"></field>
        </block>
        <block type="integer_value">
            <field name="VALUE">1</field>
        </block>
        <block type="double_value">
            <field name="VALUE">NaN</field>
        </block>
        <block type="boolean_value">
            <field name="VALUE">true</field>
        </block>
        <block type="color_value">
            <field name="VALUE">#ffff00</field>
        </block>
        <block type="hsl">
            <value name="VALUE_1">
                <block type="integer_value">
                    <field name="VALUE">128</field>
                </block>
            </value>
            <value name="VALUE_2">
                <block type="integer_value">
                    <field name="VALUE">128</field>
                </block>
            </value>
            <value name="VALUE_3">
                <block type="integer_value">
                    <field name="VALUE">128</field>
                </block>
            </value>
        </block>
        <block type="tuple_value"></block>
        <block type="tuple_3_value"></block>
        <block type="random_value">
            <value name="MAX">
                <block type="integer_value">
                    <field name="VALUE">100</field>
                </block>
            </value>
        </block>
        <block type="random_value_between">
            <value name="MIN">
                <block type="integer_value">
                        <field name="VALUE">10</field>
                    </block>
            </value>
            <value name="MAX">
                <block type="integer_value">
                        <field name="VALUE">100</field>
                    </block>
            </value>
        </block>
    </category>
    <category name="Types" colour="#a55b80">
        <block type="class_integer">
            <field name="NAME">Integer</field>
        </block>
        <block type="class_double">
            <field name="NAME">Double</field>
        </block>
        <block type="class_boolean">
            <field name="NAME">Boolean</field>
        </block>
        <block type="class_string">
            <field name="NAME">String</field>
        </block>
        <block type="class_other">
            <field name="NAME"></field>
        </block>
    </category>
    <category name="Functions" colour="#a5745b" custom="Functions">
        <block type="return"></block>
        <block type="lambda_1_param"></block>
        <block type="lambda_2_param"></block>
        <block type="function">
            <field name="FUNCTION_NAME">functionName</field>
            <statement name="BODY">
                <block type="return"></block>
            </statement>
        </block>
        <block type="function_1_param">
            <field name="FUNCTION_NAME">functionName</field>
            <field name="PARAM_NAME">x</field>
            <statement name="BODY">
                <block type="return"></block>
            </statement>
        </block>
        <block type="function_2_param">
            <field name="FUNCTION_NAME">functionName</field>
            <field name="PARAM_1_NAME">x</field>
            <field name="PARAM_2_NAME">y</field>
            <statement name="BODY">
                <block type="return"></block>
            </statement>
        </block>
        <block type="function_3_param">
            <field name="FUNCTION_NAME">functionName</field>
            <field name="PARAM_1_NAME">x</field>
            <field name="PARAM_2_NAME">y</field>
            <field name="PARAM_3_NAME">z</field>
            <statement name="BODY">
                <block type="return"></block>
            </statement>
        </block>
    </category>
    <category name="Definitions" colour="#9fa55b" custom="Definitions">
        <block type="define">
            <field name="NAME">x</field>
        </block>
        <block type="val">
            <field name="NAME">x</field>
        </block>
        <block type="var">
            <field name="NAME">x</field>
        </block>
    </category>
</xml>